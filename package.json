{
  "name": "yuzha-yo",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "description": "-",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "yuzha",
    "meng",
    "counter2"
  ],
  "scripts": {
    "dev": "concurrently -k -n yuzha,meng,counter2 \"npm run dev:yuzha\" \"npm run dev:meng\" \"npm run dev:counter2\"",
    "dev:3000": "npm run dev:3000 --workspace yuzha",
    "dev:5000": "npm run dev:5000 --workspace yuzha",
    "dev:debug": "DEBUG=vite:* npm run dev --workspace yuzha",
    "dev:host": "npm run dev:yuzha -- --host",
    "preview": "npm run preview --workspace yuzha",
    "preview:3000": "npm run preview:3000 --workspace yuzha",
    "preview:5000": "npm run preview:5000 --workspace yuzha",
    "preview:prod": "npm run build && npm run preview",
    "build": "npm run build --workspace yuzha",
    "build:fast": "NODE_ENV=production npm run build --workspace yuzha",
    "build:analyze": "npm run build -- --mode analyze",
    "clean": "rimraf \"yuzha/dist\" \"yuzha/node_modules/.vite\" --glob \"**/.tsbuildinfo\"",
    "clean:cache": "rimraf \"node_modules/.vite\" \"yuzha/node_modules/.vite\" --glob \"**/.tsbuildinfo\"",
    "clean:all": "rimraf \"node_modules\" \"yuzha/node_modules\" \"yuzha/dist\" --glob \"**/.tsbuildinfo\"",
    "typecheck": "npm run typecheck --workspace yuzha && tsc --noEmit --project tsconfig.base.json",
    "typecheck:shared": "tsc --noEmit --project tsconfig.base.json",
    "ci:verify": "npm run lint && npm run typecheck && npm run test && npm run build",
    "lint": "eslint \"yuzha/**/*.{ts,tsx}\" \"shared/**/*.{ts,tsx}\" --cache --no-error-on-unmatched-pattern --max-warnings=0",
    "lint:fix": "eslint \"yuzha/**/*.{ts,tsx}\" \"shared/**/*.{ts,tsx}\" --cache --no-error-on-unmatched-pattern --fix",
    "lint:shared": "eslint \"shared/**/*.{ts,tsx}\" --cache --no-error-on-unmatched-pattern --max-warnings=0",
    "lint:shared:fix": "eslint \"shared/**/*.{ts,tsx}\" --cache --no-error-on-unmatched-pattern --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "test:cov": "vitest run --passWithNoTests --coverage",
    "dev:l": "npm run typecheck && npm run lint && npm run format:fix",
    "dev:shared": "npm run typecheck:shared && npm run lint:shared && npm run test:shared",
    "dev:shared:fix": "npm run typecheck:shared && npm run lint:shared:fix && npm run format:fix && npm run test:shared",
    "sync:image": "node -e \"import(\\\"./shared/asset/ImageRegistry.mjs\\\").then(m => m.syncImageRegistry())\"",
    "deps:update": "npm update && npm outdated",
    "deps:audit": "npm audit && npm audit fix",
    "start": "npm run preview:5000",
    "dev:yuzha": "npm run dev --workspace yuzha",
    "dev:meng": "npm run dev --workspace meng",
    "build:meng": "npm run build --workspace meng",
    "lint:meng": "npm run lint --workspace meng",
    "typecheck:meng": "npm run typecheck --workspace meng",
    "preview:meng": "npm run preview --workspace meng",
    "dev:counter2": "npm run dev --workspace counter2",
    "build:counter2": "npm run build --workspace counter2",
    "preview:counter2": "npm run preview --workspace counter2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.8.29",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^7.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@faker-js/faker": "^10.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.556.0",
    "react-day-picker": "^9.12.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.180.0",
    "vaul": "^1.1.2"
  }
}
